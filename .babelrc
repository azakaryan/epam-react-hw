{
  "presets": ["@babel/preset-env", "@babel/preset-react"],

  "plugins": ["@babel/syntax-dynamic-import", "@babel/plugin-proposal-class-properties"],

  "env": {
    "test": {
      "plugins": [
        "dynamic-import-node",
        ["transform-es2015-modules-commonjs", {"allowTopLevelThis": true}]
      ]
    }
  }
}